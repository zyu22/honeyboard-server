<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.honeyboard.api.generation.mapper.GenerationMapper">

    <select id="selectGenerations" resultType="Generation">
        select id, name, is_active as active
        from generation
    </select>

    <select id="selectUserByGeneration" resultType="User">
        select id,
               email,
               name,
               generation_id,
               is_ssafy
        from user
        where generation_id
    </select>
</mapper>